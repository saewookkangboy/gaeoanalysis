{
  "name": "gaeo-analysis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "bash scripts/cleanup-dev.sh && next dev",
    "cleanup": "bash scripts/cleanup-dev.sh",
    "cleanup:all": "bash scripts/cleanup-dev.sh --clean",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "npx tsx scripts/migrate-db.ts",
    "db:migrate-to-postgres": "npx tsx scripts/migrate-sqlite-to-postgres.ts",
    "db:backup": "bash scripts/backup-db.sh",
    "db:restore": "bash scripts/restore-db.sh",
    "db:optimize": "npx tsx scripts/optimize-db.ts",
    "db:download-from-vercel": "npx tsx scripts/download-db-from-vercel.ts",
    "db:download-from-vercel:cli": "bash scripts/download-db-vercel-cli.sh",
    "db:upload-to-railway": "bash scripts/upload-db-to-railway.sh",
    "firebase:delete-user": "npx tsx scripts/delete-firebase-user.ts",
    "check:auth": "bash scripts/check-auth-secret.sh",
    "check:env": "npx tsx scripts/check-env.ts",
    "test:github-oauth": "npx tsx scripts/test-github-oauth.ts",
    "test:user-flow": "npx tsx scripts/test-user-flow.ts",
    "test:user-flow:prod": "TEST_ENV=production npx tsx scripts/test-user-flow.ts",
    "stats:aggregate": "npx tsx scripts/daily-statistics-aggregator.ts",
    "push": "bash scripts/auto-push.sh"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/pg": "^8.15.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "better-sqlite3": "^12.5.0",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "firebase": "^12.6.0",
    "highlight.js": "^11.11.1",
    "next": "16.0.6",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "firebase-admin": "^13.6.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.17",
    "@tailwindcss/oxide-linux-x64-musl": "^4.1.17",
    "lightningcss-linux-x64-gnu": "^1.30.2"
  }
}
